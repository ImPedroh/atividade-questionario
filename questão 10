    <h1>Senhas</h1>
    <script>
        let senha =parseInt(prompt("Digite a sua senha aqui"))

        if(senha >=1235){
            document.write("ACESSO NEGADO")
        }
        else if(senha <=1233){
            document.write("ACESSO NEGADO")
        }
        else if(senha =1234);{
            document.write("ACESSO PERMITIDO")
        }
    </script>
